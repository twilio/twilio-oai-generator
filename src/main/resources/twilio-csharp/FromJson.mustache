
        /// <summary>
        /// Converts a JSON string into a {{apiName}}{{resourceConstant}} object
        /// </summary>
        /// <param name="json"> Raw JSON string </param>
        /// <returns> {{apiName}}{{resourceConstant}} object represented by the provided JSON </returns>
        public static {{apiName}}{{resourceConstant}} FromJson(string json)
        {
            try
            {
                return JsonConvert.DeserializeObject<{{apiName}}{{resourceConstant}}>(json);
            }
            catch (JsonException e)
            {
                throw new ApiException(e.Message, e);
            }
        }